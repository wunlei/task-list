{
  "env": {
    "browser": true
  },
  "extends": ["eslint:recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "prefer-const": "error",
    "no-else-return": "error",
    "object-shorthand": "error",
    "no-useless-return": "error",
    "no-shadow": "error"
  },
  "root": true
}
